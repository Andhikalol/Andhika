import os
import random
import sys
import time
os.system("clear")
def mengetik(s):
    for c in s + '\n':
        sys.stdout.write(c)
        sys.stdout.flush()
        time.sleep(random.random() * 0.3)
mengetik("Terima kasih sayang, waktu berjalan dan kamu tetap memilihku menjadi seseorang terspesial di hidupmu. Aku akan menjaga cinta kita hingga waktu yang memisahkan")

print ("")

