import os
import random
import sys
import time
os.system("clear")
def mengetik(s):
    for c in s + '\n':
        sys.stdout.write(c)
        sys.stdout.flush()
        time.sleep(random.random() * 0.3)
mengetik("wah nggak sangka kamu menerima aku sebagai pacarmu, aku akan berjanji tetap setia dan memberikan yang terbaik untukmu")