import os
import random
import sys
import time
os.system("clear")
def mengetik(s):
    for c in s + '\n':
        sys.stdout.write(c)
        sys.stdout.flush()
        time.sleep(random.random() * 0.3)
mengetik("1.saya tidak akan pernah memutuskan pacar saya")

def mengetik(s):
    for c in s + '\n':
        sys.stdout.write(c)
        sys.stdout.flush()
        time.sleep(random.random() * 0.3)
mengetik("2.saya akan menjadikan ratu untuk orang yang menerima saya")

def mengetik(s):
    for c in s + '\n':
        sys.stdout.write(c)
        sys.stdout.flush()
        time.sleep(random.random() * 0.3)
mengetik("3.saya akan mengalah")

def mengetik(s):
    for c in s + '\n':
        sys.stdout.write(c)
        sys.stdout.flush()
        time.sleep(random.random() * 0.3)
mengetik("4.jika menurut saya anda tidak menghargai saya ,akan merubah sifat saya")

