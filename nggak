import os
import random
import sys
import time
os.system("clear")
def mengetik(s):
    for c in s + '\n':
        sys.stdout.write(c)
        sys.stdout.flush()
        time.sleep(random.random() * 0.3)
mengetik("yahh sayang sekali terimakasih buat semuanya kamu sangat berarti semoga kedepannya kita berteman saja ya")